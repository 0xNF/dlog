{
    "targets": [
        {
            "name": "logdebug",
            "type": "Debug"
        },
        {
            "name": "logfile",
            "type": "File",
            "fileName": "file.txt"
        },
        {
            "name": "logconsole",
            "type": "Console"
        }
    ],
    "rules": [
        {
            "name": "*",
            "minlevel": "Info",
            "writeTo": "logconsole"
        },
        {
            "name": "*",
            "minlevel": "Debug",
            "writeTo": "logfile"
        },
        {
            "name": "*",
            "minlevel": "Debug",
            "writeTo": "logdebug"
        }
    ],
    "variables": []
}